<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.moontv.fi" name="MoonTV.fi" version="0.1" provider-name="Jani (rasjani) Mikkonen">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.xbmcswift2" version="1.2.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.0" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language>fi</language>
    <summary>Finnish Net-TV Channel MoonTV</summary>
    <description>Finnish Net-TV Channel MoonTV</description>
    <summary lang="fi">Suomalainen netti tv kanava MoonTV</summary>
    <description lang="fi">Suomalainen netti tv kanava MoonTV</description>
    <summary lang="en">Finnish Net-TV Channel MoonTV</summary>
    <description lang="en">Finnish Net-TV Channel MoonTV</description>
  </extension>
</addon>
<addon id="plugin.video.sciflix" name="SciFlix" version="0.2" provider-name="Jani (rasjani) Mikkonen">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.xbmcswift2" version="1.1.1"/>
    <import addon="script.module.beautifulsoup" version="3.0.8"/>
    <import addon="plugin.video.youtube" version="3.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language></language>
    <summary>SciFlix dot blogspot dot com</summary>
    <description>Your Source For Best Free Scifi and Fantasy Flicks on Internet</description>
  </extension>
</addon>
<addon id="repository.rasjanisrepo.xbmc" name="Rasjani's Add-ons" version="1.0.0" provider-name="rasjani">
        <extension point="xbmc.addon.repository" name="Rasjani's Add-ons">
                <info compressed="false">http://pcuf.fi/~rasjani/xbmc-rasjanisrepo/addons.xml</info>
                <checksum>http://pcuf.fi/~rasjani/xbmc-rasjanisrepo/addons.xml.md5</checksum>
                <datadir zip="true">http://pcuf.fi/~rasjani/xbmc-rasjanisrepo/</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
                <summary>Finnish free streaming  content and some general addons</summary>
                <summary lang="fi">Ilmaista suomalaista sisältöä ja muita lisäreitä.</summary>
                <description>Provides freely available content from Finnish media sites where no official xbmc addons available.</description>
                <description lang="fi">Lisärit tuo suomalaisen sisällön sivuilta joilla ei ole virallista xbmc lisäriä.</description>
                <disclaimer></disclaimer>
                <platform>all</platform>
        </extension>
</addon>

</addons>
